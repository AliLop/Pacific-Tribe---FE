ist.map((music, index) => {\r\n        \r\n                    return (\r\n                        <div className='track-bloc' key={index}>\r\n                        <h6>{music.track}</h6>\r\n                        <h5>{music.artist}</h5>\r\n                        <AudioPlayer className=\"audio-compo\"\r\n                            // autoPlay\r\n                            src={music.preview_url}\r\n                            onPlay={e => console.log(\"onPlay\")}\r\n                            showSkipControls=\"true\"\r\n                            // other props here\r\n                        />\r\n                     \r\n                          {/*\r\n                        <audio controls>\r\n                                <h5>Track title</h5>\r\n                                <h6>Artist</h6>\r\n                                <source src={preview_url} type=\"audio/mpeg\" />\r\n                        </audio>\r\n                      \r\n                        <SpotifyPlayer\r\n                            uri={preview_url}\r\n                            size= \"compact\"\r\n                            view='list'\r\n                            theme='white'\r\n                            className='spotify-player'\r\n                        />\r\n                            \r\n                       Other approach: use the audio\r\n                        <audio controls>\r\n                                <source src={preview_url} type=\"audio/mp3\" />\r\n                        </audio>\r\n                        */}\r\n\r\n                      </div> \r\n                    )\r\n               })}   \r\n            </div> \r\n        )\r\n    }\r\n}\r\n\r\nexport default MusicDaily;\r\n\r\n","C:\\users\\alici\\desktop\\labs\\Moduel3-FINAL-PROJECT-FE\\src\\utils\\sapi.js",[],{"ruleId":"96","severity":1,"message":"97","line":37,"column":11,"nodeType":"98","endLine":37,"endColumn":47},{"ruleId":"96","severity":1,"message":"99","line":58,"column"
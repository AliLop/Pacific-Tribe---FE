itation';\n\nclass App extends React.Component {\n\n  state = {\n    loggedInUser: null,\n    userId: '',\n    userName: ''\n  }\n\n  componentDidMount () {\n    if (this.state.loggedInUser === null);\n    const authService = new AuthService();\n    // checkin if the user session is still active on the server.\n   authService.loggedin()\n   .then((response) => {\n     if (response.data._id) {\n       // there's a user session active then set the state\n       // with the current user.\n       console.log(\"this is the value of response data id\", response.data._id)\n       this.setCurrentUser(response.data);\n       localStorage.setItem(\"loggedInUser\", response.data._id);\n       localStorage.setItem(\"loggedInUsername\", response.data.username);\n     } else {\n       localStorage.removeItem('loggedInUser');\n       localStorage.removeItem(\"loggedInUsername\");\n     }\n   })\n }  \n\n  setCurrentUser = (user) => {\n    this.setState({\n      loggedInUser: user,\n      userId: user._id,\n      userName: user.username\n    })\n  }\n\n\n  render () {\n      return (\n        <div className=\"App\">\n        <ToastContainer />\n          <Navbar loggedInUser={this.state.loggedInUser} setCurrentUser={this.setCurrentUser}/>\n          <Switch>\n            <Route exact path=\"/\" component={Homepage} />\n            <Route path='/signup' component={Signup} />\n            <Route path='/login' render={\n              () => {\n                return < Login setCurrentUser={this.setCurrentUser}/>\n              }\n            } />\n            <Route path=\"/login-google\" render={\n              () => {\n                window.location.href = `${process.env.REACT_APP_PROJECT_API}/api/auth/google`\n              }\n            } />\n\n            <Route exact path=\"/daily-mood\" render={\n              () => {\n                if (localStorage.getItem('loggedInUser')) {\n                  return <DailyMood loggedInUser={this.state.loggedInUser} userId={this.state.userId} />\n                } else {\n                  return <Redirect to='/login' />\n              }\n            }\n            }/>\n            <Route  path={`/moodboard/${this.state.userId}`} component={Moodboard} userId={this.state.userId} userName={this.state.userName}/>\n        \n            {/* <Route exact path=\"/music-daily\" component={MusicDaily} /> */}\n           {/* <Route exact path=\"/yoga-video\" component={YogaVideo} />\n            <Route exact path=\"/meditation-video\" component={MeditationVideo} />  */}\n          </Switch>\n          <Footer />\n        </div>\n      );\n      }\n    }\n\nexport default App;\n","C:\\Users\\salom\\Desktop\\IronHack-project3\\Project3\\Project3\\project3-FE\\src\\utils\\auth.js",[],"C:\\Users\\salom\\Desktop\\IronHack-project3\\Project3\\Project3\\project3-FE\\src\\utils\\mapi.js",[],"C:\\Users\\salom\\Desktop\\IronHack-project3\\Project3\\Project3\\project3-FE\\src\\components\\Main\\Moodboard.js",["93","94","95","96","97"],"import MusicDaily from '../Suggestions/MusicDaily';\nimport YogaVideo from '../Suggestions/Yoga';\nimport MeditationVideo from '../Suggestions/Meditation';\nimport Inspiration from '../Suggestions/Inspiration'\nimport MoodService from '../../utils/mapi'\nimport Sentence from '../Suggestions/Sentence';\nimport Graph from '../Main/Graph';\nimport EvasionRoom from '../Suggestions/EvasionRoom';\nimport React from 'react';\nclass Moodboard extends React.Component {\n\n    state = {\n          mood: '',\n          spotifyURI: '',\n          sentences: [],\n          meditationURL: [],\n          yogaURL:[],\n          coachingURL: [],\n          inspirationURL: []\n          //userName: this.props.userName\n    }\n\n  \n     componentDidMount () {\n         //Function to retrieve the mood of the day (the last one from the array)\n         const moodService = new MoodService();\n         const userId = localStorage.getItem('loggedInUser'); \n        moodService.getTheMoodOfTheDay(userId)\n       